    "@babel/preset-env": "^7.10.4",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "moment": "^2.29.4",
    "npm-run-all": "^4.1.5",
    "pg": "^8.3.0"
  }
}
